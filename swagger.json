	"basePath": "/users",
	"paths":{
		"/signup":{
			"post":{
				"summary" : "create users",
				"description":"use to singn up some information",
				"produces":"application/json",
				
				"requestBody":{
				    "description":"create user object",
					"content":{
						"application/json":{
							"schema":{"$ref":"#/component/schemas/user"}
						}
					}
				},
				"responses":{
